
public class MultiSum {

	public static void main(String[] args) {
		MultiSum sum = new MultiSum();
		System.out.println(sum.addMult());
	}
	
	public int addMult()	{
		int one = 3;
		int two = 9;
		int answer;
		boolean cond = false;
			if (cond = true)	{
				answer = one + two;
			}
			else	{
				answer = one * two;
			}
		return answer;
	}

}
