
public class Car extends Vehicle {
	public boolean isConvertable;
	public int wheelSize;

	public Car(boolean isConvertable, int wheelSize, int engineSize, int wheelCount) {
		super(engineSize, wheelCount);
		this.isConvertable = isConvertable;
		this.wheelSize = wheelSize;
	}

	public String toString() {
		return "Engine Size: " + super.engineSize + ", Wheels: " + super.wheelCount + ", Wheel Size: " + this.wheelSize + ", Convertable? " + this.isConvertable;
	}

}
