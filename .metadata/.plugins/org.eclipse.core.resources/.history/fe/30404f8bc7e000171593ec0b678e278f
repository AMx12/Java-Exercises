
public class Boat extends Vehicle {
	public boolean hasSail;
	public int rudderSize;

	public Boat(int engineSize, int wheelCount, int rudderSize, boolean hasSail, int price) {
		super(engineSize, wheelCount, price);
		this.hasSail = hasSail;
		this.rudderSize = rudderSize;
	}

	public String toString() {
		return "Boat -- Engine Size: " + super.engineSize + ", Wheels: " + super.wheelCount + ", Rudder size: "
				+ this.rudderSize + ", Sail? " + this.hasSail + ", Price: " + this.price;
	}
}
