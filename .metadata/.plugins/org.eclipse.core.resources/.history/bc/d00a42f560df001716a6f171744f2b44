
public class Car extends Vehicle {
	public boolean isConvertable;
	public int wheelSize;

	public Car(int engineSize, int wheelCount, int wheelSize, boolean isConvertable, int price) {
		super(engineSize, wheelCount, price);
		this.isConvertable = isConvertable;
		this.wheelSize = wheelSize;
		price = 3;
	}

	public String toString() {
		return "Engine Size: " + super.engineSize + ", Wheels: " + super.wheelCount + ", Wheel Size: " + this.wheelSize
				+ ", Convertable? " + this.isConvertable + ", Price: " + this.price;
	}

}
