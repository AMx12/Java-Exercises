
public class FindFibonacci {

	public static void main(String[] args) {
		FindFibonacci result = new FindFibonacci();
		System.out.println(result.Fibonacci(1));
		System.out.println(result.Fibonacci(2));
		System.out.println(result.Fibonacci(3));
		System.out.println(result.Fibonacci(4));
		System.out.println(result.Fibonacci(5));
		System.out.println(result.Fibonacci(6));
		// 1 = 1
		// 2 = 1
		// 3 = 2
		// 4 = 3
		// 5 = 5
		// 6 = 8
	}

	public int Fibonacci(int numberInput) {
		int fibonacciNumber = 1;
		int fibonacciLast = 0;

		if (numberInput == 0) {
			fibonacciNumber = 0;
		} else {
			for (int i = 1; i < numberInput; i++) {
				fibonacciLast = i;
				fibonacciNumber = fibonacciNumber + fibonacciLast;
			}
		}

		return fibonacciNumber;
	}
}
