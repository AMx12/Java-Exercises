		FindFibonacci result = new FindFibonacci();
		// System.out.println(result.Fibonacci(0)); // 0
		// System.out.println(result.Fibonacci(1)); // 1
		// System.out.println(result.Fibonacci(2)); // 1
		// System.out.println(result.Fibonacci(3)); // 2
		// System.out.println(result.Fibonacci(4)); // 3
		// System.out.println(result.Fibonacci(5)); // 5
		// System.out.println(result.Fibonacci(6)); // 8
		// System.out.println(result.Fibonacci(7)); // 13
		// System.out.println(result.Fibonacci(8)); // 21
		// System.out.println(result.Fibonacci(9)); // 34
		// System.out.println(result.Fibonacci(10)); // 55
		// System.out.println(result.Fibonacci(11)); // 89
		// System.out.println(result.Fibonacci(12)); // 144
		System.out.println(result.Fibonacci(13)); // 233
	}

	public int Fibonacci(int numberInput) {
		int fibonacciSecondPrevious = 0;
		int fibonacciPrevious = 1;
		int fibonacciNumber = 1;
		String theMaths = "";

		if (numberInput == 0) {
			fibonacciNumber = 0;
		} else {
			for (int i = 1; i < numberInput; i++) {
				fibonacciNumber = fibonacciPrevious + fibonacciSecondPrevious;
				theMaths += fibonacciPrevious + " + ";
				fibonacciSecondPrevious = fibonacciPrevious;
				fibonacciPrevious = fibonacciNumber;
			}
		}
		System.out.println(theMaths);
		return fibonacciNumber;
	}
}
