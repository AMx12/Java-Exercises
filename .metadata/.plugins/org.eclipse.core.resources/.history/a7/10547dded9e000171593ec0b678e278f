
public abstract class Vehicle {
	// Initialises a number of common variables all vehicles will have
	public int engineSize;
	public int wheelCount;
	public int price;

	public Vehicle(int engineSize, int wheelCount) {
		this.engineSize = engineSize;
		this.wheelCount = wheelCount;
	}

	public int getPrice() {
		price = engineSize + (wheelCount * 50);
		return price;
	}

	public void setPrice(int price) {
		this.price = price;
	}
}